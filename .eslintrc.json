{
    "extends": ["airbnb-base", "prettier"],
    "rules": {
        "prefer-destructuring": "off",
        "prettier/prettier": "error",
        "no-undef": "off",
        "no-unsafe-finally": "off",
        "class-methods-use-this": "off",
        "no-var": "off",
        "guard-for-in": "off",
        "global-require": "off"
    },
    "plugins": ["prettier"],
    "parser": "babel-eslint"
}